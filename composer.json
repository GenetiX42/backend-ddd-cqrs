{
    "name": "fulll/vehicle-fleet-management",
    "description": "Vehicle fleet parking management - DDD/CQRS Step 2",
    "type": "project",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/runtime": "^7.3"
    },
    "require-dev": {
        "behat/behat": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Infra\\": "src/Infra/",
            "UserInterface\\": "src/UserInterface/"
        },
        "classmap": ["src/Kernel.php"]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
